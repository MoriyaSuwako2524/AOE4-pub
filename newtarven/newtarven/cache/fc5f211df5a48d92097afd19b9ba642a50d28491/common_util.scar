-- 获得指定时代的单位
function Util_GetBpNameByAge(bpName, age)
	for civ, bps in pairs(_squads) do 
		if bps[bpName] ~= nil then 
			local bp = bps[bpName] 
			if bp.age > age then 
				-- 不能升级
				return nil
			end
			for _, bpInfo in pairs(bps) do 
				-- 找到相同key并且相同时代的单位
				if bp.baseId == bpInfo.baseId and bpInfo.age == age then 
					return bpInfo.attribName
				end
			end
		end
	end
	return nil
end


function Util_GetRaceBySBP(bpName)
	for civ, bps in pairs(_squads) do 
		if bps[bpName] ~= nil then 
			return civ
		end
	end
	return nil
end


-- 获得更高时代的单位
function Util_GetBpNameByHigherAge(bpName)
	local race = Util_GetRaceBySBP(bpName)
	local bps = _squads[race]
	if bps ~= nil and bps[bpName] ~= nil then 
		local bp = bps[bpName]
		local bpNames = {}
		for _, bpInfo in pairs(bps) do 
			-- 找到相同key并且相同时代的单位
			if bp.baseId == bpInfo.baseId and bpInfo.age == (bp.age + 1) then 
				table.insert(bpNames, bpInfo.attribName)
			end
		end
		if #bpNames > 0 then 
			table.sort(bpNames)
			return bpNames[1]
		end
	end

	return nil
end



-- 格式化秒数成 00:00 
function Util_FormatSecs(sec)
	local time = "00"
	if sec > 60 then 
		local mins = math.floor(sec/60)
		if mins < 10 then 
			time = "0" .. mins 
		else 
			time = mins
		end
	end
	
	local s = math.ceil(sec%60)
	if s < 10 then 
	  s = "0" .. s	
	end
	
	time = time .. ":" ..  s
	-- print(string.format("%s --- %s", sec, time))
	return time
end

